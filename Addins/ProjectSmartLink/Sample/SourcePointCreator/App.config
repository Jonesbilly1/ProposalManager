<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <!-- The Client ID of the registered application -->
    <add key="ClientId" value="[CLIENT_ID]"/>
    <add key="Authority" value="https://login.microsoftonline.com/common"/>
    <!--The URL of the Project Smart Link API-->
    <add key="ApiUrl" value="[PROJECT_SMARTLINK_API_URL]"/>
    <!-- The custom redirect URL configured for the native application platform in the registered application -->
    <add key="RedirectUri" value="[REDIRECT_URL"/>
  </appSettings>
</configuration>